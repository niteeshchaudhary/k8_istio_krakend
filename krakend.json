{
    "version": 3,
    "extra_config": {
      "github_com/devopsfaith/krakend-websocket": {
        "endpoints": [
          {
            "endpoint": "/ws",
            "backend": [
              {
                "url_pattern": "/",
                "host": ["ws://ws-server.krakend-test.svc.cluster.local"]
              }
            ]
          }
        ]
      }
    },
    "endpoints": [
      {
        "endpoint": "/health",
        "method": "GET",
        "backend": [
          {
            "url_pattern": "/",
            "host": ["http://ws-server.krakend-test.svc.cluster.local"]
          }
        ]
      }
    ]
  }
  